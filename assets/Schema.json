{
    "issuer": "Binance",
    "desc": "A Crypto exchange and swap platform",
    "website": "https://www.binance.com/my/dashboard",
    "APIs": [
      {
        "host": "www.binance.com",
        "intercept": {
          "url": "bapi/accounts/v1/private/account/user/base-detail",
          "method": "POST"
        },
        "nullifier": "data|userId"
      },
      {
        "host": "www.binance.com",
        "intercept": {
          "url": "bapi/apex/v1/private/apex/marketing/userTradeStatus",
          "method": "POST"
        },
        "assert": [
          {
            "key": "data|hasTrade",
            "value": "true",
            "operation": "=="
          }
        ]
      }
    ],
    "HRCondition": [
      "User has a Binance Account and has made a trade"
    ],
    "tips": {
      "message": "When you successfully log in, please click the 'Start' button to initiate the verification process."
    }
  }